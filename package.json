  },
  "dependencies": {
    "check-if-windows": "^1.0.0",
    "regex-extname-posix": "^1.0.0",
    "regex-extname-windows": "^1.0.0"
  },
  "devDependencies": {
    "chai": "3.x.x",
    "codecov.io": "^0.1.5",
    "istanbul": "^0.3.0",
    "jshint": "2.x.x",
    "jshint-stylish": "2.x.x",
